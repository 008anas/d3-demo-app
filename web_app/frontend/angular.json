{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "sqrutiny": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "sqy",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "outputPath": "public",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "node_modules/open-vector-editor/umd/main.css",
              "node_modules/ng-zorro-antd/ng-zorro-antd.min.css",
              "semantic/dist/semantic.min.css",
              "src/assets/styles/helper.scss",
              "src/styles.scss"
            ],
            "scripts": [
              "node_modules/d3/dist/d3.min.js",
              "node_modules/protvista-utils/dist/protvista-utils.js",
              "node_modules/protvista-zoomable/dist/protvista-zoomable.js",
              "node_modules/protvista-track/dist/protvista-track.js",
              "src/vendor/sqrutiny-matrix/dist/sqrutiny-matrix.js",
              "node_modules/protvista-navigation/dist/protvista-navigation.js",
              "node_modules/protvista-sequence/dist/protvista-sequence.js",
              "src/vendor/protvista-manager/dist/protvista-manager.js",
              "node_modules/protvista-interpro-track/dist/protvista-interpro-track.js",
              "node_modules/open-vector-editor/umd/open-vector-editor.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "7mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "options": {
            "browserTarget": "sqrutiny:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "sqrutiny:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "sqrutiny:build"
          }
        },
        "test": {
          "builder": "ngx-build-plus:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "sqrutiny:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "sqrutiny:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "sqrutiny"
}
